<main class="main-wrap">

    <section class="content-main">
        <div class="content-header">
            <h2 class="content-title">Coupons list</h2>
            <div>
                <a href="/admin/add_coupon" class="btn btn-primary"><i class="material-icons md-plus"></i> Create coupon</a>
            </div>
        </div>
        <div class="card mb-4">
            <header class="card-header">
                <div class="row gx-3">
                    <div class="col-lg-4 col-md-6 me-auto">
                        <input type="text" placeholder="Search..." class="form-control">
                    </div>

                    <div class="col-lg-2 col-md-3 col-6">
                        {{!-- <select class="form-select">
                            <option>Show 20</option>
                            <option>Show 30</option>
                            <option>Show 40</option>
                        </select> --}}
                    </div>
                </div>
            </header> <!-- card-header end// -->
            <div class="card-body ">
                <div class="table-responsive ">
                    <table class="table table-hover ">
                        <thead>
                            <tr>
                                <th class="text-center">Coupon Code</th>
                                <th class="text-center">Discount Percentage</th>
                                <th class="text-center">Expiry Date</th>
                                <th class="text-center">Action</th>
                            </tr>
                        </thead>
                         {{#each couponData}}
                        <tbody>
                            <tr >
                                <td width="20%" class="text-center">
                                        <div class="info pl-3">
                                            <h6 class="mb-0 title">{{this._doc.code}}</h6>
                                        </div>
                                </td>
                                <td class="text-center" width="20%">{{this._doc.discount}}%</td>
                                 <td class="text-center" width="20%">{{this.expiryDate}}</td>


                                <td class="text-center" width="20%">
                                    <a href="/admin/delete_cpn?id={{this._doc._id}}" id = "delete" onclick="return confirm ('Are you sure want to delete?')" type="button" class="btn btn-primary">
                                        Delete
                                    </a>
                                </td>

                            </tr>

                        </tbody>
                        {{/each}}
                    </table> <!-- table-responsive.// -->
                </div>
            </div> <!-- card-body end// -->
        </div> <!-- card end// -->
<div class="pagination-area mt-30 mb-50">
    <nav aria-label="Page navigation example">
        <ul class="pagination justify-content-start">
            {{#each pages}}
                <li class="page-number {{#if (eq ../currentPage this)}}active{{/if}}">
                    <a class="page-link" href="?page={{this}}">{{this}}</a>
                </li>
            {{/each}}
        </ul>
    </nav>
</div>


        
